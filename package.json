{
  "name": "markdown-it-music",
  "version": "0.0.19",
  "description": "Renderer for Music Markdown.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": "./dist/index.js",
    "./bin/*": "./dist/bin/*.js",
    "./lib/*": "./dist/lib/*.js",
    "./parsers/*": "./dist/parsers/*.js",
    "./renderers/*": "./dist/renderers/*.js",
    "./package.json": "./package.json"
  },
  "scripts": {
    "build": "tsc",
    "test": "jest",
    "lint": "eslint src",
    "posttest": "eslint src",
    "fix-lint": "eslint src --fix"
  },
  "jest": {
    "roots": [
      "src"
    ],
    "testMatch": [
      "**/?(*.)+(spec|test).+(ts|tsx|js)"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "ts-jest"
    }
  },
  "keywords": [
    "markdown-it-plugin"
  ],
  "author": "Shad Sharma <shadanan@gmail.com>",
  "license": "ISC",
  "bin": {
    "musicmd": "dist/bin/musicmd.js"
  },
  "dependencies": {
    "@svgdotjs/svg.js": "^3.0.16",
    "abcjs": "^5.12.0",
    "fast-memoize": "^2.5.2",
    "jsdom-global": "^3.0.2",
    "markdown-it": "^12.0.4",
    "markdown-it-fence": "^0.1.3",
    "markdown-it-meta": "0.0.1",
    "svgdom": "^0.1.8",
    "vextab": "^3.0.6",
    "yargs": "^15.4.1"
  },
  "devDependencies": {
    "@types/jest": "^26.0.20",
    "@types/markdown-it": "^12.0.1",
    "@types/rewire": "^2.5.28",
    "babel-eslint": "^10.1.0",
    "eslint": "^7.18.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-prettier": "^3.1.3",
    "jest": "^26.6.3",
    "prettier": "^2.0.5",
    "rewire": "^5.0.0",
    "ts-jest": "^26.5.1",
    "typescript": "^4.1.4"
  }
}
